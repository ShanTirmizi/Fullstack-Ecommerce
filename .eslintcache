          <>\r\n              <Link  to={url} className='navbar__info'>\r\n                <BottomNavigationAction key={id}   className={classes.root}\r\n                style={{fontSize: '1.5rem'}} icon={icon} />\r\n                <p className='navbar__info__p'>\r\n                  {text}\r\n                </p>\r\n              </Link>\r\n            </>\r\n\r\n          )\r\n         })\r\n       }\r\n       {\r\n         myUser ? (\r\n          <Link to='/checkout' className='navbar__info'>\r\n            <BottomNavigationAction className={classes.root}\r\n              style={{fontSize: '1.5rem'}} icon={<FaMoneyCheck />}/>\r\n              <p className='navbar__info__p'>\r\n                Checkout\r\n              </p>\r\n          </Link>\r\n         ) : (\r\n          <Link onClick={loginWithRedirect} className='navbar__info'>\r\n            <BottomNavigationAction className={classes.root}\r\n              style={{fontSize: '1.5rem'}} icon={<FaMoneyCheck />}/>\r\n              <p className='navbar__info__p'>\r\n                Checkout\r\n              </p>\r\n          </Link>\r\n         )\r\n       }\r\n\r\n          {\r\n            myUser ? (\r\n            <Link  onClick={() => {\r\n            clearCart()\r\n            logout({returnTo:window.location.origin})}\r\n          } className='navbar__info'>\r\n              <BottomNavigationAction className={classes.root}\r\n              style={{fontSize: '1.5rem'}} icon={<BsPersonDashFill />}/>\r\n              <p className='navbar__info__p'>\r\n                Logout\r\n              </p>\r\n            </Link>\r\n            ) : (\r\n              <Link className='navbar__info' onClick={loginWithRedirect}>\r\n                <BottomNavigationAction className={classes.root}\r\n                style={{fontSize: '1.5rem'}} icon={<BsFillPersonPlusFill />}/>\r\n                <p className='navbar__info__p'>\r\n                  Login\r\n                </p>\r\n            </Link>\r\n            )\r\n          }\r\n      </BottomNavigation>\r\n    </AppBar>\r\n  ) \r\n}\r\n\r\nexport default Nav\r\n","C:\\Users\\shahn\\Desktop\\Portfolio-Projects\\fullstack-ecommerce\\fullstack-ecommerce\\src\\components\\Loading.js",[],"C:\\Users\\shahn\\Desktop\\Portfolio-Projects\\fullstack-ecommerce\\fullstack-ecommerce\\src\\components\\ProductList.js",["231","232"],"import React from 'react'\r\nimport { useFilterContext } from '../context/filter_context'\r\nimport GridView from './GridView'\r\nimport ListView from './ListView'\r\n\r\nconst ProductList = () => {\r\n  const { filtered_products: products, grid_view } = useFilterContext()\r\n  if (products.length < 1) {\r\n    return (\r\n        <h5 style={{textTransform: 'none'}}>\r\n          Sorry, No product matched your search\r\n        </h5>\r\n    )\r\n  }\r\n\r\n  // if (grid_view === false) {\r\n  //   return <ListView products={products} />\r\n  // }\r\n  return <GridView products={products}>product list</GridView>\r\n}\r\n\r\nexport default ProductList\r\n","C:\\Users\\shahn\\Desktop\\Portfolio-Projects\\fullstack-ecommerce\\fullstack-ecommerce\\src\\components\\ProductImages.js",["233","234"],"import React, { useState, useEffect } from 'react'\r\nimport styled from 'styled-components'\r\nimport './ProductImages.css'\r\nimport { useParams } from 'react-router-dom';\r\nimport {dogData} from '../utils/constants'\r\n\r\n\r\nconst ProductImages = ({ images = [{ url:''}] }) => {\r\n  let { id } = useParams();\r\n  const mainData = dogData.find((x) => x.id === id)\r\n  const mainImg = mainData.images[0]\r\n\r\n  const [main, setMain] = useState(mainImg)\r\n\r\n\r\n  return (\r\n    <div className='product__images'>\r\n      <img src={main.url === '' ?  images[0].url : main.url} alt='main pic' className='product__main' />\r\n      {/* <img src={images[0].url} alt='main pic' className='product__main' /> */}\r\n      <div className=\"product__secondary\">\r\n        {\r\n          images.slice(0,3).map((image, index) => {\r\n            return (\r\n              <img src={image.url} alt={image.filename} key={image.id} onClick={() => setMain(images[index])}\r\n                className={`${image.url === main.url && 'active'}`} />\r\n            )\r\n          })\r\n        }\r\n      </div>\r\n    </div>\r\n  ) \r\n}\r\n\r\nexport default ProductImages\r\n","C:\\Users\\shahn\\Desktop\\Portfolio-Projects\\fullstack-ecommerce\\fullstack-ecommerce\\src\\components\\Filters.js",["235","236","237","238","239","240","241","242","243"],"import React from 'react'\r\nimport styled from 'styled-components'\r\nimport { useFilterContext } from '../context/filter_context'\r\nimport { getUniqueValues, formatPrice } from '../utils/helpers'\r\nimport { FaCheck } from 'react-icons/fa'\r\nimport './Filters.css'\r\n\r\nconst Filters = () => {\r\n  const {\r\n    filters: {\r\n      text,\r\n      category,\r\n      company,\r\n      color,\r\n      min_price,\r\n      price,\r\n      max_price,\r\n      shipping,\r\n    },\r\n    updateFilters,\r\n    clearFilters,\r\n    all_products,\r\n  } = useFilterContext()\r\n\r\n  const categories = getUniqueValues(all_products, 'category')\r\n  const companies = getUniqueValues(all_products, 'company')\r\n  const colors = getUniqueValues(all_products, 'colors')\r\n  // console.log(colors)\r\n  return (\r\n    <div className=\"content__top\">\r\n      <div className=\"content\">\r\n        <form onSubmit={(e) => e.preventDefault()}>\r\n          {/* <div className='heading'>\r\n            <h1>Ecom</h1>\r\n            <div className=\"form__control__search\">\r\n              <input\r\n                type='text'\r\n                name='text'\r\n                placeholder='search'\r\n                className='search__input'\r\n                value={text}\r\n                onChange={updateFilters}\r\n              />\r\n            </div>\r\n          </div>  */}\r\n          {/* <div className='form__control'>\r\n            <h5>category</h5>\r\n            <div>\r\n              {\r\n                categories.map((cat, index) => {\r\n                  return (\r\n                    <button key={index}\r\n                      onClick={updateFilters}\r\n                      type='button'\r\n                      name='category'\r\n                      className={`${\r\n                        category === cat.toLowerCase() ? 'active' : null\r\n                      }`}>{cat}\r\n                    </button>\r\n                  ) \r\n                })\r\n              }\r\n            </div>\r\n          </div> */}\r\n          <div className='form__cat_price'>\r\n            <div className=\"form__control\">\r\n              <h3 className=\"form__control__cat\">Category</h3>\r\n              <select name='company' value={company} onChange={updateFilters} className='company'>\r\n                {\r\n                  companies.map((com, index) => {\r\n                    return (\r\n                      <option key={index} value={com}>{com}</option>\r\n                    )\r\n                  })\r\n                }\r\n              </select>\r\n            </div>\r\n            <div className=\"form__control top__price\">\r\n              <div className=\"form__control__price\">\r\n                <h3>Price</h3>\r\n                <h3 className='price'>{formatPrice(price)}</h3>\r\n              </div>\r\n              <input\r\n                type='range'\r\n                name='price'\r\n                className=\"form__control__slider\"\r\n                onChange={updateFilters}\r\n                min={min_price}\r\n                max={max_price}\r\n                value={price}\r\n                />\r\n            </div>\r\n          </div>\r\n          {/* <div className=\"form__control shipping\">\r\n            <label htmlFor='shipping'>free shipping</label>\r\n            <input\r\n              type='checkbox'\r\n              name='shipping'\r\n              id='shipping'\r\n              onChange={updateFilters}\r\n              checked={shipping} />\r\n          </div> */}\r\n        </form>\r\n        {/* <button type='button' className='clear__btn' onClick={clearFilters}>{' '} clear filters</button> */}\r\n      </div>\r\n    </div>\r\n  ) \r\n}\r\n\r\nexport default Filters\r\n","C:\\Users\\shahn\\Desktop\\Portfolio-Projects\\fullstack-ecommerce\\fullstack-ecommerce\\src\\components\\AddToCart.js",["244","245","246"],"import React, { useState } from 'react'\r\nimport styled from 'styled-components'\r\nimport { Link } from 'react-router-dom'\r\nimport { FaCheck } from 'react-icons/fa'\r\nimport { useCartContext } from '../context/cart_context'\r\nimport AmountButtons from './AmountButtons'\r\nimport './AddToCart.css'\r\n\r\nconst AddToCart = ({ item1 }) => {\r\n  const { addToCart } = useCartContext()\r\n  const { id, stock, colors } = item1\r\n  const [mainColor, setMainColor] = useState(colors[0])\r\n  const [amount, setAmount] = useState(1);\r\n\r\n  const increase = () => {\r\n    setAmount((oldAmount) => {\r\n      let amount = oldAmount + 1\r\n      if (amount > stock) {\r\n        amount = stock\r\n      }\r\n      return amount\r\n    })\r\n  }\r\n\r\n  const decrease = () => {\r\n    setAmount((oldAmount) => {\r\n      let amount = oldAmount - 1\r\n      if (amount < 1) {\r\n        amount = 1\r\n      }\r\n      return amount\r\n    })\r\n  }\r\n  return (\r\n    <div>\r\n      {/* <div className=\"colors\">\r\n        <span> colors :</span>\r\n        <div>\r\n          {\r\n            colors.map((color, index) => {\r\n              return (\r\n                <button onClick={() => setMainColor(color)} key={index} style={{ background: color }} \r\n                  className={`${mainColor === color ? 'color-btn active' : 'color-btn'}`}\r\n                  >{\r\n                    mainColor === color ? <FaCheck /> : null\r\n                    }\r\n                </button>\r\n              ) \r\n            })\r\n          }\r\n        </div>\r\n      </div> */}\r\n      <div className='add__cart'>\r\n        <AmountButtons amount={amount} increase={increase} decrease={decrease} />\r\n        <Link to='/cart' className='add__cart__btn' onClick={() => addToCart(id, mainColor, amount, item1)}>\r\n          Add To Cart\r\n        </Link>\r\n      </div>\r\n    </div>\r\n  ) \r\n}\r\n\r\n\r\nexport default AddToCart\r\n","C:\\Users\\shahn\\Desktop\\Portfolio-Projects\\fullstack-ecommerce\\fullstack-ecommerce\\src\\components\\Stars.js",["247"],"import React from 'react'\r\nimport styled from 'styled-components'\r\nimport { BsStarFill, BsStarHalf, BsStar } from 'react-icons/bs'\r\nimport './Stars.css'\r\nconst Stars = ({ stars, reviews }) => {\r\n  const tempStars = Array.from({ length: 5 }, (_, index) => {\r\n    const number = index + 0.5\r\n    return (\r\n      <span key={index}>\r\n        {\r\n          stars >= index + 1 ? (\r\n            <BsStarFill />\r\n          ) : stars >= number ? (\r\n            <BsStarHalf />\r\n          ) : (\r\n            <BsStar />\r\n          )\r\n        }\r\n      </span>\r\n    )\r\n  })\r\n  return (\r\n    <div className='stars__reviews'>\r\n      <div className=\"stars\">{tempStars}</div>\r\n      <p className='reviews'>({reviews} customer reviews)</p>\r\n    </div>\r\n  ) \r\n}\r\n\r\nexport default Stars\r\n","C:\\Users\\shahn\\Desktop\\Portfolio-Projects\\fullstack-ecommerce\\fullstack-ecommerce\\src\\components\\Sort.js",["248","249","250","251","252","253"],"import React from 'react'\r\nimport { useFilterContext } from '../context/filter_context'\r\nimport { BsFillGridFill, BsList } from 'react-icons/bs'\r\nimport styled from 'styled-components'\r\nimport './Sort.css'\r\nconst Sort = () => {\r\n  const { filtered_products: products, grid_view, setGridView, setListView, sort, updateSort } = useFilterContext()\r\n\r\n  return (\r\n    <div className='sortby'>\r\n      {/* <div className='btn-container'>\r\n        <button type='button' className={`${grid_view ? 'active' : null}`} onClick={setGridView}>\r\n          <BsFillGridFill />\r\n        </button>\r\n        <button type='button' className={`${!grid_view ? 'active' : null}`} onClick={setListView}>\r\n          <BsList />\r\n        </button>\r\n      </div> */}\r\n      <form className='sortby__content'>\r\n        <label htmlFor='sort'>Sort by </label>\r\n        <select name='sort' id='sort' className='sort__input' value={sort} onChange={updateSort}>\r\n          <option value='price-lowest'>price (lowest)</option>\r\n          <option value='price-highest'>price (highest)</option>\r\n          <option value='name-a'>name (a-z)</option>\r\n          <option value='name-z'>name (z-a)</option>\r\n        </select>\r\n      </form>\r\n      <p className='sortby__amount'>{products.length} products found</p>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Sort\r\n","C:\\Users\\shahn\\Desktop\\Portfolio-Projects\\fullstack-ecommerce\\fullstack-ecommerce\\src\\components\\StripeCheckout.js",[],"C:\\Users\\shahn\\Desktop\\Portfolio-Projects\\fullstack-ecommerce\\fullstack-ecommerce\\src\\components\\PageHero.js",[],"C:\\Users\\shahn\\Desktop\\Portfolio-Projects\\fullstack-ecommerce\\fullstack-ecommerce\\src\\utils\\helpers.js",[],"C:\\Users\\shahn\\Desktop\\Portfolio-Projects\\fullstack-ecommerce\\fullstack-ecommerce\\src\\components\\CartTotals.js",["254"],"import React from 'react'\r\nimport styled from 'styled-components'\r\nimport { useCartContext } from '../context/cart_context'\r\nimport { useUserContext } from '../context/user_context'\r\nimport { formatPrice } from '../utils/helpers'\r\nimport { Link } from 'react-router-dom'\r\nimport './CartTotals.css'\r\n\r\nconst CartTotals = () => {\r\n  const { total_amount, shipping_fee } = useCartContext()\r\n  const { myUser, loginWithRedirect } = useUserContext()\r\n\r\n  return (\r\n    <div >\r\n      <div className='cart__total'>\r\n        <article className='cart__total__info'>\r\n          <div className='cart